func applyeffect(effect, caster, target):
	
	var value = effect.value
	
	if effect.has('casterreq'):
		if globals.evaluate(effect.casterreq) == false:
			return
	if effect.has('targetreq'):
		if globals.evaluate(effect.targetreq) == false:
			return
	
	

